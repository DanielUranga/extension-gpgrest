<?xml version="1.0" encoding="UTF-8"?>

<Defaults>

	<Text>

		<Title
			filters="[new flash.filters.DropShadowFilter()]"
			label-selectable="false"
			mouseEnabled="false"
			format-size="30"
			format-color="0xffffff"
			format-bold="true"
		/>

		<PlayerName
			label-selectable="false"
			mouseEnabled="false"
			format-size="25"
			format-font="'Arial'"
			format-color="0x000000"
		/>

		<PlayerRank
			label-selectable="false"
			mouseEnabled="false"
			format-size="24"
			format-font="'Arial'"
			format-color="0x000000"
		/>

		<PlayerScore
			label-selectable="false"
			mouseEnabled="false"
			format-size="20"
			format-font="'Arial'"
			format-color="Stablex.color2"
		/>

		<AchievementsDefault
			label-selectable="false"
			mouseEnabled="false"
			format-size="22"
			format-color="0x333333"
			format-font="'Arial'"
		/>

	</Text>

	<TextWithMaxHeight>
		<AchievementsDefault
			label-selectable="false"
			mouseEnabled="false"
			format-size="22"
			format-color="0xbbbbbb"
			format-font="'Arial'"
		/>
	</TextWithMaxHeight>

</Defaults>
